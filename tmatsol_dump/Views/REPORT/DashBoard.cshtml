                    X_HTML += "<table style='color:black;font-size: 12px;'>";
                    X_HTML += "<tr><th>Cashier</th><th>In DT</th><th>Out DT</th></tr>";

                    obj.CIOData.forEach(myFunction);

                    function myFunction(item) {
                        X_HTML += "<tr>";
                        X_HTML += "<td>" + item.Cashier + "</td>";
                        X_HTML += "<td>" + item.In_DT + "</td>";
                        X_HTML += "<td>" + item.Out_DT + "</td>";
                        X_HTML += "</tr>";
                    }
                    X_HTML += "</table>";
                }
                X_HTML += "<hr/>";
                X_HTML += "<h5 style='color:black'><b> Running Order</b></h5>";
                X_HTML += "<hr/>";

                X_HTML += "<h6 style='color:black'><b> Dinein : </b>" + Number(obj.Dinein).toFixed(2) + "</h6>";
                X_HTML += "<h6 style='color:black'><b> Takeaway : </b>" + Number(obj.Takeaway).toFixed(2) + "</h6>";
                X_HTML += "<h6 style='color:black'><b> Delivery : </b>" + Number(obj.Delivery).toFixed(2) + "</h6>";
                X_HTML += "<h6 style='color:black'><b> Corporate : </b>" + Number(obj.Corporate).toFixed(2) + "</h6>";
                X_HTML += "<h6 style='color:black'><b> Drive Th. : </b>" + Number(obj.Drive_Th).toFixed(2) + "</h6>";

                if (obj.Day_Last_Invoice_Detail !== "") {
                    X_HTML += "<h6 style='color:black'><b> Day Last Invoice Detail(Number,DateTime) : </b>" + obj.Day_Last_Invoice_Detail + "</h6>";
                }
                if (obj.ToDayLast_Sync_DT !== null) {
                    X_HTML += "<h6 style='color:black'><b> Last Sync : </b>" + obj.ToDayLast_Sync_DT + "</h6>";
                }
                //-----------------Html---------------------------
                $("#Html1").append(X_HTML);
            }
        });
    }
</script>

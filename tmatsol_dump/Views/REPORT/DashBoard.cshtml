            document.cookie = "TableId=none;expires=Fri, 31 Dec 2100 12:00:00 UTC;path=/";
        }

    })
    function getCookie(cname) {
        let name = cname + "=";
        let ca = document.cookie.split(';');
        for (let i = 0; i < ca.length; i++) {
            let c = ca[i];
            while (c.charAt(0) == ' ') {
                c = c.substring(1);
            }
            if (c.indexOf(name) == 0) {
                return c.substring(name.length, c.length);
            }
        }
        return "";
    }
    var preloader = document.getElementById('loading');

    //window.onbeforeunload = function (event) {
    //    return confirm("Confirm refresh");
    //};
    setInterval(function () {
        Page_Refresh();
    }, 30000);//evry 20 sec....

    function Page_Refresh() {
        var Custid = getCookie("id");
        var CustName = getCookie("Comp_Name");

        $.ajax({
            type: "get",
            url: "/REPORT/DashBoard_RELOAD",
            data: { 'Custid': Custid, 'CustName': CustName },
            dataType: "json",
            traditional: true,
            success: function (mydata) {

                var obj = JSON.parse(mydata);
                $("#TotalOrders").html("");
                $("#Earnings").html("");
                $("#Expenses").html("");
                $("#CancelOrders").html("");
                $("#RunningOrders").html("");
                $("#Total").html("");
                $("#Html1").html("");
               
                obj.ReportResult_Dataset.ReportResult_Table.forEach(myFunctionLevel1);

                var Dinein , Takeaway , Delivery , Corporate , Drive_Th ;

                function myFunctionLevel1(item) {
                    if (item.Action_Name == "Total Earning") {
                        $("#Earnings").text(Number(item.Action_Amount).toFixed(2));
                    }
                    else if (item.Action_Name == "Total Runnig invoice Amt") {
                        $("#RunningOrders").text(Number(item.Action_Amount).toFixed(2));
                    }
                    else if (item.Action_Name == "Total Expense Amt") {
                        $("#Expenses").text(Number(item.Action_Amount).toFixed(2));
                    }
                    else if (item.Action_Name == "Total Orders") {
                        $("#TotalOrders").text(Number(item.Action_Amount).toFixed(2));
                    }
                    else if (item.Action_Name == "Total Void Items") {
                        $("#CancelOrders").text(Number(item.Action_Amount).toFixed(2));
                    }
                    else if (item.Action_Name == "Running DieIn Amt") {                       
                        Dinein = item.Action_Amount;                       
                    }
                    else if (item.Action_Name == "Running TakeAway Amt") {
                        Takeaway=item.Action_Amount;
                    }
                    else if (item.Action_Name == "Running Delivery Amt") {
                        Delivery=item.Action_Amount;
                    }
                    else if (item.Action_Name == "Running Corporate Amt") {
                        Corporate=item.Action_Amount;
                    }
                    else if (item.Action_Name == "Running DriveThrough Amt") {
                        Drive_Th = item.Action_Amount;
                    }
                }
                var a1 = document.getElementById("Earnings");
                var b1 = document.getElementById("RunningOrders");

                var a = a1.innerHTML;
                var b = b1.innerHTML;
                var c = Number(a) + Number(b);

                $("#Total").text(Number(c).toFixed(2));
                //debugger;
                //-----------------Html---------------------------
                var X_HTML="";
                var X_HTML2 = "";
                if (obj.CIOData !== null) {
                    X_HTML += "<table style='color:black;font-size: 12px;'>";
                    X_HTML += "<tr><th>Cashier</th><th>In DT , Out DT</th></tr>";

                    obj.ReportResult_Dataset.ReportResult_Table_CIO.forEach(myFunction);
                    function myFunction(item) {                       
                        X_HTML += "<tr>";
                        X_HTML += "<td>" + item.Action_Name + "</td>";
                        X_HTML += "<td>" + item.Action_Result + "</td>";
                        X_HTML += "</tr>";
                    }                   
                    X_HTML += "</table>";
                }

                X_HTML += "<hr/>";
                X_HTML += "<h5 style='color:black'><b> Running Order</b></h5>";
                X_HTML += "<hr/>";             
                X_HTML += "<h6 style='color:black'><b> Dinein : </b>" + Number(Dinein).toFixed(2) + "</h6>";
                X_HTML += "<h6 style='color:black'><b> Takeaway : </b>" + Number(Takeaway).toFixed(2) + "</h6>";
                X_HTML += "<h6 style='color:black'><b> Delivery : </b>" + Number(Delivery).toFixed(2) + "</h6>";
                X_HTML += "<h6 style='color:black'><b> Corporate : </b>" + Number(Corporate).toFixed(2) + "</h6>";
                X_HTML += "<h6 style='color:black'><b> Drive Th. : </b>" + Number(Drive_Th).toFixed(2) + "</h6>";
                
                //if (obj.Day_Last_Invoice_Detail!=="") {
                //    X_HTML += "<h6 style='color:black'><b> Day Last Invoice Detail(Number,DateTime) : </b>" + obj.Day_Last_Invoice_Detail + "</h6>";
                //}
                //if (obj.ToDayLast_Sync_DT !== null) {
                //    X_HTML += "<h6 style='color:black'><b> Last Sync : </b>" + obj.ToDayLast_Sync_DT + "</h6>";
                //}
                //-----------------Html---------------------------
                $("#Html1").append(X_HTML);
            }
        });
    }

@model tmatsol_dump.fi_usermast
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Report Utility</title>
    <link rel="icon" href="~/images/icon1.png" />
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.5.0/css/all.css" />
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @*@RenderSection("headerscripts", required: false)*@
    @Scripts.Render("~/bundles/jquery")
</head>

<body onload="getid()">
    <section id="services" class="container">
        <!--Deck1-->

        <div class="card-deck justify-content-around">
            <div>
                @using (Html.BeginForm())
                {
                    @Html.AntiForgeryToken()
                    <div class="form-group">
                        <br />
                        <br />
                        <br />
                        <center><img src="~/images/think login.png" height="50%" width="50%" /></center>
                        <h6>@ViewBag.Error</h6>
                        <h6>@ViewBag.M1</h6>
                        <hr />
                        
                    </div>
                    <div class="form-group">

                        <div class="col-md-10">
                            @Html.EditorFor(model => model.Authorize_Code, new { htmlAttributes = new { @class = "form-control", placeholder = "Password", type = "Password", required = "true" } })
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-md-offset-2 col-md-10">
                            <input type="submit" class="btn btn-success" value="Submit" />
                            <input type="reset" class="btn btn-dark" value="Cancle" />
                            @Html.HiddenFor(model => model.GetCustID, new { id = "txtgetid" })
                        </div>
                    </div>
                }
            </div>
        </div>
    </section>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/jqueryval")
    <script src="~/Scripts/tmatsCustomsMain.js"></script>
    @*@RenderSection("scripts", required: false)*@
    <script>
        function getid() {
            var name = localStorage.getItem('id');
            var a = $('#txtgetid').attr('value', name)
        }
    </script>

</body>
</html>

